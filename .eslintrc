{
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "json",
        "html",
        "prettier"
    ],
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-multiple-empty-lines": "warn",
        "no-case-declarations": "off",
        "prettier/prettier": [
            "error"
        ],
        "object-curly-spacing": "off",
        "no-unreachable": "warn"
    }
}

